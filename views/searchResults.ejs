<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Player Search Results</title>
</head>
<body>
  <h1>Search Results</h1>
  <ul>
    <% players.forEach(player => { %>
      <li>
        <p>Name: <%= player.name %></p>
        <p>Age: <%= player.age %></p>
        <p>Description: <%= player.description %></p>
        <a href="/players/edit/<%= player.id %>">Edit</a>
      </li>
    <% }); %>
  </ul>
</body>
</html>
